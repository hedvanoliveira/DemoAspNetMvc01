@model IEnumerable<DemoAspNetMvc01.Domain.Model.Post>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<ul>
    @foreach (var item in Model)
    {
        <li>
            <h3>@Html.ActionLink(item.Title, "Details", new { id = item.Id })</h3>
            <p>@Html.DisplayFor(modelItem => item.Description)</p>
        </li>

    }
</ul>

@Html.Action("_ListUserPartialView", "User")
@Html.Action("_ListCategoryPartialView", "Category")